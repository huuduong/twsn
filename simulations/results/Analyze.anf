<?xml version="1.0" encoding="UTF-8"?>
<scave:Analysis xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:scave="http://www.omnetpp.org/omnetpp/scave">
  <inputs>
    <inputs name="/TWSN/simulations/results/Config2-*.vec"/>
    <inputs name="/TWSN/simulations/results/Config2-*.sca"/>
    <inputs name="*.vec"/>
    <inputs name="*.sca"/>
  </inputs>
  <datasets>
    <datasets name="R">
      <items xsi:type="scave:Add" filterPattern="meaError:histogram" type="HISTOGRAM"/>
      <items xsi:type="scave:HistogramChart" name="Measurement of distance error distribution">
        <properties name="Graph.Title" value="Measurement of distance error distribution"/>
        <properties name="Bars.Baseline" value="-100.0"/>
        <properties name="Hist.Bar" value="Outline"/>
        <properties name="Hist.Data" value="Count"/>
        <properties name="Hist.ShowOverflowCell" value="true"/>
      </items>
      <items xsi:type="scave:Add" filterPattern="meaError:vector" type="VECTOR"/>
      <items xsi:type="scave:LineChart" name="Measurement error of distance over time" lineNameFormat="">
        <properties name="Graph.Title" value="Measurement error of distance over time"/>
        <properties name="Y.Axis.Min" value="-20.0"/>
        <properties name="Y.Axis.Max" value="20.0"/>
        <properties name="Symbols.Size" value="2"/>
        <properties name="Line.Type" value="Dots"/>
      </items>
    </datasets>
  </datasets>
  <chartSheets>
    <chartSheets name="default" charts="//@datasets/@datasets.0/@items.3"/>
  </chartSheets>
</scave:Analysis>
